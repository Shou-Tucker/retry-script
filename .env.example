# リトライスクリプト環境変数設定例
# このファイルを.envにコピーして使用してください

# ユーザー設定 (最大5ユーザー)
# ユーザー1
USER1_TENANCY_OCID=your-tenancy-ocid-1
USER1_USER_OCID=your-user-ocid-1
USER1_FINGERPRINT=your-fingerprint-1
USER1_PRIVATE_KEY_PATH=~/.oci/user1/private.pem
USER1_REGION=ap-tokyo-1

# ユーザー2
USER2_TENANCY_OCID=your-tenancy-ocid-2
USER2_USER_OCID=your-user-ocid-2
USER2_FINGERPRINT=your-fingerprint-2
USER2_PRIVATE_KEY_PATH=~/.oci/user2/private.pem
USER2_REGION=ap-tokyo-1

# ユーザー3
USER3_TENANCY_OCID=your-tenancy-ocid-3
USER3_USER_OCID=your-user-ocid-3
USER3_FINGERPRINT=your-fingerprint-3
USER3_PRIVATE_KEY_PATH=~/.oci/user3/private.pem
USER3_REGION=ap-tokyo-1

# ユーザー4
USER4_TENANCY_OCID=your-tenancy-ocid-4
USER4_USER_OCID=your-user-ocid-4
USER4_FINGERPRINT=your-fingerprint-4
USER4_PRIVATE_KEY_PATH=~/.oci/user4/private.pem
USER4_REGION=ap-tokyo-1

# ユーザー5
USER5_TENANCY_OCID=your-tenancy-ocid-5
USER5_USER_OCID=your-user-ocid-5
USER5_FINGERPRINT=your-fingerprint-5
USER5_PRIVATE_KEY_PATH=~/.oci/user5/private.pem
USER5_REGION=ap-tokyo-1

# 共通OCI設定
OCI_COMPARTMENT_ID=your-compartment-id
OCI_AVAILABILITY_DOMAIN=your-availability-domain
OCI_SUBNET_ID=your-subnet-id
OCI_IMAGE_ID=your-image-id

# 通知設定
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your-webhook-url
SLACK_WEBHOOK=https://hooks.slack.com/services/your-slack-webhook

# その他の設定
TZ=Asia/Tokyo

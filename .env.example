# リトライスクリプト環境変数設定ファイル
# 使用方法: このファイルを.envにコピーして値を設定します

# ユーザー設定 (5つのユーザー用)
# ユーザー1
USER1_NAME=user1
USER1_EMAIL=user1@example.com
USER1_API_KEY=your-api-key-1
USER1_SECRET=your-secret-1
USER1_REGION=ap-northeast-1

# ユーザー2
USER2_NAME=user2
USER2_EMAIL=user2@example.com
USER2_API_KEY=your-api-key-2
USER2_SECRET=your-secret-2
USER2_REGION=ap-northeast-1

# ユーザー3
USER3_NAME=user3
USER3_EMAIL=user3@example.com
USER3_API_KEY=your-api-key-3
USER3_SECRET=your-secret-3
USER3_REGION=ap-northeast-1

# ユーザー4
USER4_NAME=user4
USER4_EMAIL=user4@example.com
USER4_API_KEY=your-api-key-4
USER4_SECRET=your-secret-4
USER4_REGION=ap-northeast-1

# ユーザー5
USER5_NAME=user5
USER5_EMAIL=user5@example.com
USER5_API_KEY=your-api-key-5
USER5_SECRET=your-secret-5
USER5_REGION=ap-northeast-1

# OCI設定
OCI_TENANCY_OCID=your-tenancy-ocid
OCI_USER_OCID=your-user-ocid
OCI_FINGERPRINT=your-fingerprint
OCI_PRIVATE_KEY_PATH=~/.oci/private.pem
OCI_REGION=ap-tokyo-1
OCI_COMPARTMENT_ID=your-compartment-id
OCI_AVAILABILITY_DOMAIN=your-availability-domain
OCI_SUBNET_ID=your-subnet-id
OCI_IMAGE_ID=your-image-id

# 通知設定
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your-webhook-url
SLACK_WEBHOOK=https://hooks.slack.com/services/your-slack-webhook

# リトライ設定
RETRY_INTERVAL=30
RETRY_BACKOFF=1.5
RETRY_MAX_INTERVAL=300
RETRY_TIMEOUT=600
